import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n,p=1;
        n=sc.nextInt();
        while(n>=p)
        {
            if(n==p)
            {
                System.out.print("YES");
                return;
            }
            p=p*2;
        }
        System.out.print("NO");
    }
}
