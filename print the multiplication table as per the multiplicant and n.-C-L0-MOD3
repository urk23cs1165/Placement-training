import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a,b,i;
        a = sc.nextInt();
        b = sc.nextInt();
        for(i=1;i<=a;i++)
        {
            System.out.println(i+" * "+b+" = "+i*b);
        }
        
    }
}
